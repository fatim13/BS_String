class Solution {
    public int searchInsert(int[] nums, int target) {
         int n = nums.length;

         int l = 0;
         int h = n-1;

         while(l<=h){
             int mid = l + (h - l)/2;
             if(nums[mid]==target){
                 return mid;
             }else if(nums[mid]>target){
                 h = mid - 1;
             }else{
                 l = mid + 1;
             }
         }

         return l;  //is the index at which element should be there.
    }
}

//TC:nlogn